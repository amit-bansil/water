package cps.water;

import ca.odell.glazedlists.BasicEventList;
import ca.odell.glazedlists.EventList;
import cps.water.data.DataViewerModel;

/**
 * <p>TODO document Model
 * </p>
 * @version $Id$
 * @author Amit Bansil
 */
public class ModelManager {

	final EventList<Model> simModels;
	final DataViewerModel dataViewer1Model,dataViewer2Model;
	public ModelManager() {
		models=new BasicEventList<Model>();
		dataViewer1Model=new DataViewerModel(simModels);
		dataViewer2Model=new DataViewerModel(simModels);
	}
}
